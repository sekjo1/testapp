<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akra.testapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/zielscheibe"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/zielscheibe"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActivityStartGame"
            android:screenOrientation="sensorPortrait"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".ActivityGameplayTest"
            android:screenOrientation="sensorPortrait"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".UpgradeInterfaceActivity"
            android:screenOrientation="sensorPortrait"
            android:label="Shoot It!">
        </activity>
        <activity android:name=".MainActivity"
            android:screenOrientation="sensorPortrait"
            android:label="Shoot It!">
        </activity>
        <activity android:name=".RegisterActivity"
            android:screenOrientation="sensorPortrait"
        android:label="Shoot It!">
        </activity>
        <activity android:name=".HighscoreActivity"
            android:screenOrientation="sensorPortrait"
            android:label="Shoot It!">
        </activity>
    </application>

</manifest>